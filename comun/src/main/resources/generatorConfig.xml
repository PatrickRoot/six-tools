<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >

    <context id="context1"  targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>

        <!-- jdbc连接信息 -->
        <commentGenerator><property name="suppressAllComments" value="true" /></commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/six_tools?useUnicode=true&amp;characterEncoding=UTF-8" userId="test" password="test" />

        <!--配置实体bean-->
        <javaModelGenerator targetPackage="org.eu.sixlab.sixtools.comun.bean" targetProject="./temp" />
        <!--配置Mapper的xml-->
        <sqlMapGenerator targetPackage="org.eu.sixlab.sixtools.comun.mapper" targetProject="./temp" />
        <!--配置Mapper接口的java-->
        <javaClientGenerator targetPackage="org.eu.sixlab.sixtools.comun.mapper" targetProject="./temp" type="XMLMAPPER" />

        <table tableName="six_plan" domainObjectName="SixPlan" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="JDBC"
                          identity="true"></generatedKey>
        </table>
        <table tableName="six_tools" domainObjectName="SixTools" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="JDBC"
                          identity="true"></generatedKey>
        </table>
        <table tableName="six_tray" domainObjectName="SixTray" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="JDBC"
                          identity="true"></generatedKey>
        </table>
        <table tableName="movie_record" domainObjectName="MovieRecord" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <generatedKey column="id" sqlStatement="JDBC"
                          identity="true"></generatedKey>
        </table>

    </context>
</generatorConfiguration>